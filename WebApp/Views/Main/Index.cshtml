
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<section class="rooms rooms-widget">

    <!-- === rooms header === -->

    <div class="section-header" style="direction: rtl;padding-bottom:141px">
        <div class="container-fluid" style="display: flex">
            <div class="logo col-xl-2 col-lg-2 col-md-2 col-xs-2 col-2  ">
                    <img class="img-fluid" src="~/img/logo.png" alt="fayoum-logo" title="fayoum-logo" style="width: 129px;height:101px" />    
            </div>
            <div class="col-xl-8 col-lg-8 col-md-8 col-xs-8 col-8">
                <h2 class="section-title">جدول أعمال الاجتماع رقم @ViewBag.currentsessionnum </h2>
                <h4 class="section-title">لمجلس إدارة شركة مياه الشرب والصرف الصحى بالفيوم</h4>
                <h4 class="section-title">المنعقد يوم @ViewBag.currentsessionday <span></span>    الموافق: <span> @ViewBag.sessiondate</span></h4>
            </div>
            <div class="logo col-xl-2 col-lg-2 col-md-2 col-xs-2 col-2  ">

            </div>

        </div>
    </div>



    <!-- === rooms content === -->

    <div class="container">

        <div class="owl-rooms owl-themes">

            <!-- === rooms item === -->

            <div class="item">
                <article>
                    <div class="image">
                        <img src="~/img/services-4.jpg" alt="" />
                    </div>
                    <div class="details">
                        <div class="text">
                            <h3 class="title"><a href="room-overview.html">مذكرات الجلسة</a></h3>
                            <p>اجتماع رقم <span>"@ViewBag.currentsessionnum"</span></p>
                        </div>
                        <div class="book">
                            <div>
                                <a href="@Url.Action("ViewSub","Memo", new { id = @ViewBag.sessionid, sessionNum =@ViewBag.currentsessionnum})" class="btn btn-main">عـرض</a>
                            </div>
                        </div>
                    </div>
                </article>
            </div>
            <!-- === rooms item === -->
            <div class="item">
                <article>
                    <div class="image">
                        <img src="~/img/services-3.jpg" alt="" />
                    </div>
                    <div class="details">
                        <div class="text">
                            <h3 class="title"><a href="">الاوامر الادارية لجلسة مجلس الادارة</a></h3>
                            <p>اجتماع رقم <span>"@ViewBag.prevsessionnum"</span></p>
                        </div>
                        <div class="book">
                            <div>
                                <a href="~/FileUpload/@ViewBag.instract" class="btn btn-main">عـرض</a>
                            </div>
                        </div>
                    </div>
                </article>
            </div>


            <!-- === rooms item === -->

            <div class="item">
                <article>
                    <div class="image">
                        <img src="~/img/services-2.jpg" alt="" />
                    </div>
                    <div class="details">
                        <div class="text">
                            <h3 class="title"><a href="room-overview.html">جدول متابعة قرارات مجلس الادارة</a></h3>
                            <p>اجتماع رقم  <span>"@ViewBag.prevsessionnum"</span></p>
                        </div>
                        <div class="book">
                            <div>
                                <a href="~/FileUpload/@ViewBag.sch" class="btn btn-main">عـرض</a>
                            </div>
                        </div>
                    </div>
                </article>
            </div>

            <!-- === rooms item === -->
            <div class="item">
                <article>
                    <div class="image">
                        <img src="~/img/services-1.jpg" alt="" />
                    </div>
                    <div class="details">
                        <div class="text">
                            <h4 class="title"><a href="room-overview.html">التصديق على محضر مجلس الادارة</a></h4>
                            <p>اجتماع رقم  <span>"@ViewBag.prevsessionnum"</span></p>
                        </div>
                        <div class="book">
                            <div>
                                <a href="~/FileUpload/@ViewBag.rep" class="btn btn-main">عـرض</a>
                            </div>

                        </div>
                    </div>
                </article>
            </div>


        </div><!--/owl-rooms-->

    </div> <!--/container-->




</section>




