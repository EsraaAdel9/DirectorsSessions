<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/material-bootstrap-wizard.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
</head>
<body>
    <div class="image-container set-full-height" style="background-image: url('/img/wizard-city.jpg') ">
        <!--   Big container   -->
        <div class="container">
            <div class="row">
                <div class="col-sm-10 col-sm-offset-1">
                    <!--      Wizard container        -->
                    <div class="wizard-container">
                        <div class="card wizard-card" data-color="red" id="wizard">
                            <!--        You can switch " data-color="rose" "  with one of the next bright colors: "blue", "green", "orange", "purple"        -->
                           

                            <div style="max-width:909px;padding:0 22px 0 13px">
                                <div class="row">

                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div> <!-- wizard container -->
                </div>
            </div> <!-- row -->
        </div> <!--  big container -->
        <!--Footer-->
        <div class="footer">
            <div class="container text-center">
                <i></i>تكنولوجيا المعلومات - ادارة التحول الرقمى <a href="http://www.creative-tim.com"></a>
            </div>
        </div>
        <script src="~/Scripts/jquery.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/Scripts/material-bootstrap-wizard.js"></script>
        <script src="~/Scripts/jquery.bootstrap.js"></script>
    </div>
</body>
</html>
